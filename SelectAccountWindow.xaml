<Window x:Class="AtmApplicationWPF.SelectAccountWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Select Account" Height="300" Width="300">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <TextBox x:Name="AccountNumberTextBox" Width="200" Margin="10" />
            <Button x:Name="SelectButton" Content="Select" Width="200" Margin="10" Click="SelectButton_Click"/>
        </StackPanel>
    </Grid>
</Window>
